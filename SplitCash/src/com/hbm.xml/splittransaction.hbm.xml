<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.entity.SplitTransaction" table="splittransaction">
		<composite-id>
			<key-property name="transaction_id" column="transaction_id" length="10"  />
			<key-property name="user_id" column="user_id" length="10" />
		</composite-id>
		<property name="product_id" column="product_id" not-null="true" length="10"/>
		<property name="amount" column="amount" not-null="true" length="10"/>
		<property name="paid" column="paid" not-null="true" length="10"/>
		<property name="consent" column="consent" not-null="true" length="10"/>
		<property name="locktime" column="locktime" not-null="true" length="10"/>
	</class>

</hibernate-mapping>
